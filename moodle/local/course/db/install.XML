<XMLDB xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" PATH="local/blog/db" VERSION="20130407" COMMENT="XMLDB file for local blog plugin" xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
<TABLES>
<TABLE NAME="local_blogprestasi" COMMENT="each record is a blog">
<FIELDS>
<FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
<FIELD NAME="namakompetisi" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
<FIELD NAME="jeniskompetisi" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
<FIELD NAME="skalakompetisi" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
<FIELD NAME="penyelenggara" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
<FIELD NAME="tanggalmulai" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
<FIELD NAME="berkas" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
<FIELD NAME="status" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
</FIELDS>
<KEYS>
<KEY NAME="primary" TYPE="primary" FIELDS="id"/>
</KEYS>
</TABLE>
</TABLES>
</XMLDB>
