<div class="efb-wrap-list efb-wrap-list-form-data">
    <div class='efb-list-buttons text-right'>
        {{#pageactions}}
            <a class='btn btn-primary efb-form-icon-btn' href='{{url}}'>
                <i class="fa fa-{{icon}}" aria-hidden="true"></i>{{label}}
            </a>
        {{/pageactions}}
    </div>
    <h2>{{heading}}</h2>
    {{#submissionnotsupport}}
        {{submissionnotsupport}}
    {{/submissionnotsupport}}
    {{^submissionnotsupport}}
        {{#nodata}}
            <h4>{{nodata}}</h4>
        {{/nodata}}
        {{^nodata}}
            <table id="efb-form-submissions" class="display efb-table efb-form-submissions-table" data-formid="{{formid}}">
                <thead>
                    <tr>
                        {{#headings}}
                            <th>{{{.}}}</th>
                        {{/headings}}
                    </tr>
                </thead>
                <tbody>
                    {{#norowsmsg}}
                        <tr>
                            <td colspan="6">{{norowsmsg}}</td>
                        </tr>
                    {{/norowsmsg}}
                </tbody>
            </table>
        {{/nodata}}
    {{/submissionnotsupport}}
</div>
